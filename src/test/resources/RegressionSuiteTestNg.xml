<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Regression Suite">
    <groups>
        <run>
            <exclude name="exclude-me"/>
        </run>
    </groups>

    <listeners>
        <listener class-name="listeners.TestListener"/>
    </listeners>

    <test name="The Internet">
        <classes>
            <class name="ui.theinternet.TheInternetTest"/>
        </classes>
    </test>

    <test name="UkrNet">
        <classes>
            <class name="ui.ukrnet.UkrnetTest"/>
        </classes>
    </test>
</suite>